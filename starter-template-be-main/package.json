    "@types/express": "^4.17.21",
    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/jws": "^3.2.10",
    "@types/lodash": "^4.17.1",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^0.7.34",
    "@types/multer": "^1.4.12",
    "@types/node": "^17.0.45",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
