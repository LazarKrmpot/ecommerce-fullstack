{
  "name": "starter-template-be",
  "version": "1.0.0",
  "description": "Starter Template BE",
  "main": "app.ts",
  "scripts": {
    "prepare": "npx husky install",
    "start:dev": "ts-node ./src/app.ts",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/app.js",
    "serve": "nodemon ./src/app.ts",
    "serve:prod": "NODE_ENV=production npm run serve",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "format:check": "prettier --check ./src/**/*.ts",
    "format:fix": "prettier --write ./src/**/*.ts",
    "lint:check": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "seed": "ts-node -r tsconfig-paths/register ./src/database/index.ts"
  },
  "author": "Andrej Kitanovic",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/swagger": "^7.3.1",
    "@typegoose/typegoose": "^11.7.1",
    "async-mutex": "^0.5.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-jsonschema": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron-typedi-decorators": "^1.0.2",
    "csv-parser": "^3.2.0",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-queue": "^0.0.13",
    "express-validator": "^6.15.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "jws": "^4.0.0",
    "lodash": "^4.17.21",
    "microframework-w3tec": "^0.6.3",
    "mongo-seeding": "^3.7.2",
    "mongoose": "^7.6.11",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "multer": "^1.4.5-lts.2",
    "redoc-express": "^2.1.0",
    "routing-controllers": "^0.10.4",
    "routing-controllers-openapi": "^4.0.0",
    "serve-favicon": "^2.5.0",
    "socket-controllers": "^0.2.0",
